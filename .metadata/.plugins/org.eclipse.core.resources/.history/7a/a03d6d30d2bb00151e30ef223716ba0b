package org.teachingkidsprogramming.CodingChallenges;

public class Print10Rowsof10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      for (int j = 0; j < 100; j++)
      {
        System.out.println();
        System.out.print((j++) + " ");
      }
    }
  }
}
