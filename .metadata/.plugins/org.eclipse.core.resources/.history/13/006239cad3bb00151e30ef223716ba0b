package org.teachingkidsprogramming.CodingChallenges;

public class Print10Rowsof10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      System.out.println();
      for (int j = 0; j < 11; j++)
      {
        System.out.print((j + 1 ) + " ");
      }
    }
  }
}
