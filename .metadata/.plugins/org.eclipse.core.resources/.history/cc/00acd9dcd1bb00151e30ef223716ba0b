package org.teachingkidsprogramming.CodingChallenges;

public class Print10Rowsof10
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 100; i++)
    {
      for (int j = 0; j < 1; j++)
      {
        System.out.print((i + 1) + " ");
        System.out.println();
      }
    }
  }
}
